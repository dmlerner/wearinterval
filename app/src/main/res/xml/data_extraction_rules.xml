<?xml version="1.0" encoding="utf-8"?>
<data-extraction-rules>
    <cloud-backup>
        <!-- Exclude sensitive data from cloud backup -->
        <exclude domain="sharedpref" path="datastore" />
    </cloud-backup>
    <device-transfer>
        <!-- Allow user configurations to transfer between devices -->
        <include domain="sharedpref" path="datastore" />
    </device-transfer>
</data-extraction-rules>